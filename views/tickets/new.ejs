<%- include('../partials/header') %>

<form id="new-form" action="/flights/<%= flight._id %>/tickets/new" method="POST">
  <label>Seat: </label>
    <input type="string" name="seat">
  <label>Price: </label>
<input type="number" name="price" required min="10">
  <label>Flight No.:</label>
  <input type="number" name="flightNo" required min="10" max="9999">
  <input type="submit" value="Add Tickets">
</form>

